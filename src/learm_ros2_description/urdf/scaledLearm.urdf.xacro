<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro"  name="robot">

    <!-- This will include all the contents of example_include.xacro first. -->
    <xacro:include filename="example_include.xacro" />

    <!-- This first link called "world" is empty -->
    <link name="world"></link>

    <joint name="base_joint" type="fixed">
        <origin xyz="0.015 0.010 0" rpy="0 0 0"/>
        <parent link="world"/>
        <child link="base_link"/>        
    </joint>

    <link name="base_link">
        <visual>
            <origin xyz="0 0 0.035" rpy="0 0 0"/>
            <geometry>
                <box size="0.16 0.29 0.07" />
            </geometry>
            <material name="green">
                <color rgba="0.2 1 0.2 1"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0.035" rpy="0 0 0"/>
            <geometry>
                <box size="0.16 0.29 0.07" />
            </geometry>
        </collision>
        <xacro:inertial_box mass="500" x="0.16" y="0.29" z="0.07">
            <origin xyz="0 0 0.035" rpy="0 0 0"/>
        </xacro:inertial_box>
    </link>

    <joint name="shoulder_pan" type="revolute">
        <origin xyz="0 0.07 0.07" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child link="shoulder_link"/>
        <axis xyz="0 0 1"/>
        <limit lower="${-pi}" upper="${pi}" velocity="100" effort="100"/> 
    </joint>

    <link name="shoulder_link">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <box size="0.042 0.041 0.022" />
            </geometry>
            <material name="black">
                <color rgba="0 0 0 0.91"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <box size="0.042 0.041 0.022" />
            </geometry>
        </collision>
        <xacro:inertial_box mass="50" x="0.042" y="0.041" z="0.022">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:inertial_box>
    </link>

    <joint name="shoulder_lift" type="revolute">
        <origin xyz="0 0.0105 0.011" rpy="0 0 0"/>
        <parent link="shoulder_link"/>
        <child link="humerus_link"/>
        <axis xyz="-1 0 0"/>
        <limit lower="${-pi/2}" upper="${pi/2}" velocity="100" effort="100"/> 
    </joint>

    <link name="humerus_link">
        <visual>
            <origin xyz="0 0.015 0.07" rpy="0 0 0"/>
            <geometry>
                <box size="0.055 0.03 0.13" />
            </geometry>
            <material name="blue" />
        </visual>
        <collision>
            <origin xyz="0 0.015 0.07" rpy="0 0 0"/>
            <geometry>
                <box size="0.055 0.03 0.13" />
            </geometry>
        </collision>
        <xacro:inertial_box mass="200" x="0.055" y="0.03" z="0.13">
            <origin xyz="0 0.015 0.07" rpy="0 0 0"/>
        </xacro:inertial_box>
    </link>

    <joint name="elbow" type="revolute">
        <origin xyz="0 0.015 0.13" rpy="0 0 0"/>
        <parent link="humerus_link"/>
        <child link="forearm_link"/>
        <axis xyz="1 0 0"/>
        <limit lower="${-pi/2}" upper="${pi/2}" velocity="100" effort="100"/> 
    </joint>

    <link name="forearm_link">
        <visual>
            <origin xyz="0 0 0.055" rpy="0 0 0"/>
            <geometry>
                <box size="0.055 0.03 0.11" />
            </geometry>
            <material name="blue" />
        </visual>
        <collision>
            <origin xyz="0 0 0.055" rpy="0 0 0"/>
            <geometry>
                <box size="0.055 0.03 0.11" />
            </geometry>
        </collision>
        <xacro:inertial_box mass="200" x="0.055" y="0.03" z="0.11">
            <origin xyz="0 0 0.055" rpy="0 0 0"/>
        </xacro:inertial_box>
    </link>

    <joint name="wrist_flex" type="revolute">
        <origin xyz="0 0 0.11" rpy="0 0 0"/>
        <parent link="forearm_link"/>
        <child link="wrist_link"/>
        <axis xyz="1 0 0"/>
        <limit lower="${-pi/2}" upper="${pi/2}" velocity="100" effort="100"/> 
    </joint>

    <link name="wrist_link">
        <visual>
            <origin xyz="0 0.015 0.0325" rpy="0 0 0"/>
            <geometry>
                <box size="0.055 0.05 0.075" />
            </geometry>
            <material name="blue" />
        </visual>
        <collision>
            <origin xyz="0 0.015 0.0325" rpy="0 0 0"/>
            <geometry>
                <box size="0.055 0.05 0.075" />
            </geometry>
        </collision>
        <xacro:inertial_box mass="200" x="0.055" y="0.05" z="0.075">
            <origin xyz="0 0.015 0.0325" rpy="0 0 0"/>
        </xacro:inertial_box>
    </link>

    <joint name="wrist_roll" type="revolute">
        <origin xyz="0 0 0.075" rpy="0 0 0"/>
        <parent link="wrist_link"/>
        <child link="hand_link"/>
        <axis xyz="0 0 1"/>
        <limit effort="100" lower="${-pi/2}" upper="${pi/2}" velocity="1.0"/>
    </joint>

    <link name="hand_link">
        <visual>
            <origin xyz="0.015 -0.015 0.02" rpy="0 0 0"/>
            <geometry>
                <box size="0.05 0.055 0.04" />
            </geometry>
            <material name="blue" />
        </visual>
        <collision>
            <origin xyz="0.015 -0.015 0.02" rpy="0 0 0"/>
            <geometry>
                <box size="0.05 0.055 0.04" />
            </geometry>
        </collision>
        <xacro:inertial_box mass="200" x="0.05" y="0.055" z="0.04">
            <origin xyz="0.015 -0.015 0.02" rpy="0 0 0"/>
        </xacro:inertial_box>
    </link>

    <!-- gripper -->
    <joint name="grip_left" type="revolute">
        <parent link="hand_link"/>
        <child link="grip_left_link"/>
        <origin xyz="0 0 0.03" rpy="-1.57 0 0"/>
        <axis xyz="-1 0 0"/>
        <limit lower="-1.57" upper="0" effort="1000" velocity="0.5"/>
    </joint>

    <joint name="grip_right" type="revolute">
        <parent link="hand_link"/>
        <child link="grip_right_link"/>
        <origin xyz="0 -0.03 0.03" rpy="1.57 0 0"/>
        <axis xyz="-1 0 0"/>
        <limit lower="0" upper="1.57" effort="1000" velocity="0.5"/>
        <mimic joint="grip_left" multiplier="-1"/>
    </joint>

    <link name="grip_left_link">
        <visual>
            <origin xyz="0 0 0.03" rpy="0 0 0"/>
            <geometry>
                <box size="0.005 -0.028 0.05" />
            </geometry>
            <material name="orange" />
        </visual>
        <collision>
            <origin xyz="0 0 0.03" rpy="0 0 0"/>
            <geometry>
                <box size="0.005 -0.028 0.05" />
            </geometry>
        </collision>
        <xacro:inertial_box mass="1" x="0.005" y="-0.028" z="0.05">
            <origin xyz="0 0 0.03" rpy="0 0 0"/>
        </xacro:inertial_box>
    </link>

    <link name="grip_right_link">
        <visual>
            <origin xyz="0 0 0.03" rpy="0 0 0"/>
            <geometry>
                <box size="0.005 0.028 0.05" />
            </geometry>
            <material name="orange" />
        </visual>
        <collision>
            <origin xyz="0 0 0.03" rpy="0 0 0"/>
            <geometry>
                <box size="0.005 0.028 0.05" />
            </geometry>
        </collision>
        <xacro:inertial_box mass="1" x="0.005" y="0.028" z="0.05">
            <origin xyz="0 0 0.03" rpy="0 0 0"/>
        </xacro:inertial_box>
    </link>

    <!-- tendons -->
    <joint name="tendon_left" type="revolute">
        <parent link="hand_link"/>
        <child link="tendon_left_link"/>
        <origin xyz="0 -0.02 0.05" rpy="1.57 0 0"/>
        <axis xyz="1 0 0"/>
        <limit lower="-1.57" upper="0" effort="1000" velocity="0.5"/>
        <mimic joint="grip_left"/>
    </joint>

    <joint name="tendon_right" type="revolute">
        <parent link="hand_link"/>
        <child link="tendon_right_link"/>
        <origin xyz="0 -0.01 0.05" rpy="-1.57 0 0"/>
        <axis xyz="1 0 0"/>
        <limit lower="0" upper="1.57" effort="1000" velocity="0.5"/>
        <mimic joint="grip_left" multiplier="-1"/>
    </joint>

    <link name="tendon_left_link">
        <visual>
            <origin xyz="0 0 0.05" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.0025" length="0.038" />
            </geometry>
            <material name="orange" />
        </visual>
        <collision>
            <origin xyz="0 0 0.05" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.0025" length="0.038" />
            </geometry>
        </collision>
        <xacro:inertial_cylinder mass="1" length="0.038" radius="0.0025">
            <origin xyz="0 0 0.05" rpy="0 0 0"/>
        </xacro:inertial_cylinder>
    </link>

    <link name="tendon_right_link">
        <visual>
            <origin xyz="0 0 0.05" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.0025" length="0.038" />
            </geometry>
            <material name="orange" />
        </visual>
        <collision>
            <origin xyz="0 0 0.05" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.0025" length="0.038" />
            </geometry>
        </collision>
        <xacro:inertial_cylinder mass="1" length="0.038" radius="0.0025">
            <origin xyz="0 0 0.05" rpy="0 0 0"/>
        </xacro:inertial_cylinder>
    </link>

    <!--fingers-->
    <joint name="finger_left" type="revolute">
        <parent link="grip_left_link"/>
        <child link="finger_left_link"/>
        <origin xyz="0 0 0.055" rpy="1.57 0 0"/>
        <axis xyz="-1 0 0"/>
        <limit lower="-1.57" upper="0" effort="1000" velocity="0.5"/>
        <mimic joint="grip_left" multiplier="-1"/>
    </joint>
    <joint name="finger_right" type="revolute">
        <parent link="grip_right_link"/>
        <child link="finger_right_link"/>
        <origin xyz="0 0 0.055" rpy="-1.57 0 0"/>
        <axis xyz="-1 0 0"/>
        <limit lower="0" upper="1.57" effort="1000" velocity="0.5"/>
        <mimic joint="grip_left"/>
    </joint>

    <link name="finger_left_link">
        <visual>
            <origin xyz="0 -0.005 0.03" rpy="0 0 0"/>
            <geometry>
                <box size="0.005 0.02 0.062" />
            </geometry>
            <material name="red" />
        </visual>
        <collision>
            <origin xyz="0 -0.005 0.03" rpy="0 0 0"/>
            <geometry>
                <box size="0.005 0.02 0.062" />
            </geometry>
        </collision>
        <xacro:inertial_box mass="1" x="0.005" y="0.02" z="0.062">
            <origin xyz="0 -0.005 0.03" rpy="0 0 0"/>
        </xacro:inertial_box>
    </link>

    <link name="finger_right_link">
        <visual>
            <origin xyz="0 0.005 0.03" rpy="0 0 0"/>
            <geometry>
                <box size="0.005 -0.02 0.062" />
            </geometry>
            <material name="red" />
        </visual>
        <collision>
            <origin xyz="0 0.005 0.03" rpy="0 0 0"/>
            <geometry>
                <box size="0.005 -0.02 0.062" />
            </geometry>
        </collision>
        <xacro:inertial_box mass="1" x="0.005" y="-0.02" z="0.062">
            <origin xyz="0 0.005 0.03" rpy="0 0 0"/>
        </xacro:inertial_box>
    </link>

</robot>
